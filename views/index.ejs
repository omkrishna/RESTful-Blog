<%- include ('partials/header') %>



<% blogs.forEach(function(blog){ %>
	<div class="card">
		<h2> <%= blog.title %></h2>
		<img class="thumbnail" src="<%= blog.image %>">
		<p><%= blog.created.toDateString() %> </p>
		<a href="/blogs/<%= blog._id%>"> Read More </a>
	</div>
<% }) %>

<%- include ('partials/footer') %>
